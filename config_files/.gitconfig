[user]
	email = XXXXXXXXX
	name = Matt Fisher
[core]
	editor = vim
	autocrlf = input
	whitespace = cr-at-eol
[color]
	ui = true
[merge]
	conflictstyle = merge
[alias]
	lol = log --graph --decorate --oneline
	lola = log --graph --decorate --oneline --all
	root = rev-parse --show-toplevel
[push]
	default = simple
[pull]
  ff = only

